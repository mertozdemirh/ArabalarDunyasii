@model List<Araba>
@{
    ViewData["Title"] = "Home Page";
}

<h1>Arabalar</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Model</th>
            <th>Yıl</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.Model</td>
            <td>@item.Yil</td>
        </tr>
        }

    </tbody>
</table>